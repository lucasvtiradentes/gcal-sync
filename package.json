  },
  "main": "./dist/index.min.js",
  "scripts": {
    "prepare": "husky install",
    "start": "node ./dist/index.min.js",
    "dev": "tsx ./resources/dev.ts",
    "build_type_check": "tsc",
    "build": "rimraf ./dist && rollup -c rollup.config.js",
