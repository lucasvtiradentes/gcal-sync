{
  "name": "gcal-sync",
  "description": "ðŸ”„ a google apps script for syncing events between github to ticktick and finally both to google calendar.",
  "version": "1.5.0",
  "license": "MIT",
  "author": {
    "name": "Lucas Vieira",
    "email": "lucasvtiradentes@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvtiradentes/gcal-sync.git"
  },
  "main": "dist/GcalSync.min.js",
  "scripts": {
    "prepare": "husky install",
    "precommit": "npm run script:issues",
    "commit": "cz",
    "build:check": "tsc --noEmit",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "postbuild": "npm run scripts:postbuild",
    "pub": "semantic-release",
    "pub:check": "npm publish --dry-run",
    "pub:build": "npm pack && tar -xvzf *.tgz",
    "postversion": "echo writing new version in the dist file && npm run build",
    "dev": "ts-node ./tests/dev.ts",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --passWithNoTests --watchAll",
    "test:staged": "jest --passWithNoTests --findRelatedTests",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged --concurrent false --quiet",
    "scripts:postbuild": "ts-node ./scripts/post-build.ts",
    "script:issues": "ts-node ./scripts/github-issues.ts",
    "script:gitmoji": "ts-node ./scripts/gitmoji-setup.ts"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/google-apps-script": "^1.0.57",
    "semantic-release": "^19.0.5",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "commitizen": "^4.3.0",
    "commitizen-emoji": "^1.0.5",
    "prettier": "^2.8.4",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "lint-staged": "^13.1.0",
    "ts-jest": "^29.0.5",
    "@types/jest": "^29.2.5",
    "jest": "^29.3.1",
    "husky": "^8.0.3",
    "minify": "^7.2.2",
    "rimraf": "^4.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
